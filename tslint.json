{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty-interface": false,
        "no-shadowed-variable": false,
        "only-arrow-functions": true,
        "no-var-keyword": true,
        "eofline": false,
        "max-line-length": [true, 120],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "no-console": false
    },
    "rulesDirectory": []
}
